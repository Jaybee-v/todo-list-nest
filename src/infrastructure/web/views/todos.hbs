<html lang='en'>
  <head>
    <meta charset='UTF-8' />
    <meta name='viewport' content='width=device-width, initial-scale=1.0' />
    <title>Todo List</title>
  </head>
  <body>
    <h1>Todo List</h1>
    {{#if todos.length}}
      <ul>
        {{#each todos}}
          <li>{{this.title}}</li>
        {{/each}}
      </ul>
    {{else}}
      <p>No todos available.</p>
    {{/if}}
  </body>
</html>